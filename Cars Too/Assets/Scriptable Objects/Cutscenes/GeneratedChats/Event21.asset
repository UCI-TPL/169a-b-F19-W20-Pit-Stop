    Scenesprite: {fileID: 0}
    expression: {fileID: 11400000, guid: 76d693befe70a6146a4cfedc82daf742, type: 2}
    expression2: {fileID: 0}
    leavename: 
    voiceline: {fileID: 0}
    newbg: {fileID: 0}
  - speaker: Springtrap
    text: '...'
    Scenesprite: {fileID: 0}
    expression: {fileID: 0}
    expression2: {fileID: 0}
    leavename: 
    voiceline: {fileID: 0}
    newbg: {fileID: 0}
  - speaker: Springtrap
    text: I know.
    Scenesprite: {fileID: 0}
    expression: {fileID: 11400000, guid: 55af1010c233e11449b302fede8722f9, type: 2}
    expression2: {fileID: 0}
    leavename: 
    voiceline: {fileID: 8300000, guid: aa4e4241cef737c4f92007d00f882c3d, type: 3}
    newbg: {fileID: 0}
  - speaker: Springtrap
    text: 'I know that. '
    Scenesprite: {fileID: 0}
    expression: {fileID: 0}
