    Scenesprite: {fileID: 0}
    expression: {fileID: 0}
    expression2: {fileID: 0}
    leavename: Dex
    voiceline: {fileID: 0}
    newbg: {fileID: 0}
  - speaker: Springtrap
    text: '...'
    Scenesprite: {fileID: 0}
    expression: {fileID: 11400000, guid: 76d693befe70a6146a4cfedc82daf742, type: 2}
    expression2: {fileID: 0}
    leavename: 
    voiceline: {fileID: 0}
    newbg: {fileID: 0}
  Choice1: Comfort Dex
  Choice2: Stay with Springtrap
