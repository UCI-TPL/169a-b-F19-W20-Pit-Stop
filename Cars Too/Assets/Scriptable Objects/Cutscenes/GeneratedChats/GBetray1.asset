    expression: {fileID: 0}
    expression2: {fileID: 0}
    leavename: 
    voiceline: {fileID: 0}
    newbg: {fileID: 0}
  - speaker: Gak
    text: 'Hah! It worked! '
    Scenesprite: {fileID: 0}
    expression: {fileID: 0}
    expression2: {fileID: 0}
    leavename: 
    voiceline: {fileID: 0}
    newbg: {fileID: 0}
  - speaker: Lightning
